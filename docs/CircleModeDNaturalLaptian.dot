
graph {

layout = circo;
mindist = .1

node [shape = circle, fontname = Helvetica, margin = 0]
edge [style=invis]

subgraph 1 {
	E -- B -- Gb -- Db -- Ab -- Eb -- Bb -- F -- C -- G -- D -- A -- E
}

Db [fillcolor = gray];
Ab [fillcolor = gray];
Bb [fillcolor = gray];
F [fillcolor = gray];
G [fillcolor = gray];
D [fillcolor = cadetblue1];
A [fillcolor = gray];
}

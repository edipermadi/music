
graph {

layout = circo;
mindist = .1

node [shape = circle, fontname = Helvetica, margin = 0, style = filled]
edge [style=invis]

subgraph 1 {
	E -- F -- F# -- G -- G# -- A -- A# -- B -- C -- C# -- D -- D# -- E
}

C [fillcolor = gray];
C# [fillcolor = cadetblue1];
D [fillcolor = gray];
D# [fillcolor = white];
E [fillcolor = white];
F [fillcolor = white];
F# [fillcolor = gray];
G [fillcolor = gray];
G# [fillcolor = gray];
A [fillcolor = gray];
A# [fillcolor = white];
B [fillcolor = white];
}

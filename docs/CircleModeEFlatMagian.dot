
graph {

layout = circo;
mindist = .1

node [shape = circle, fontname = Helvetica, margin = 0, style = filled]
edge [style=invis]

subgraph 1 {
	E -- B -- Gb -- Db -- Ab -- Eb -- Bb -- F -- C -- G -- D -- A -- E
}

E [fillcolor = gray];
B [fillcolor = white];
Gb [fillcolor = gray];
Db [fillcolor = gray];
Ab [fillcolor = gray];
Eb [fillcolor = cadetblue1];
Bb [fillcolor = gray];
F [fillcolor = gray];
C [fillcolor = white];
G [fillcolor = white];
D [fillcolor = white];
A [fillcolor = white];
}
